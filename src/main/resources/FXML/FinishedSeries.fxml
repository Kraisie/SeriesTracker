<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane id="anchorPane" prefHeight="590.0" prefWidth="400.0" stylesheets="@../css/myStyle.css"
            xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.Kraisie.SceneController.FinishedController">
    <TableView fx:id="tableFinishedSeries" layoutX="25.0" layoutY="47.0" prefHeight="440.0" prefWidth="350.0">
        <columns>
            <TableColumn fx:id="columnName" prefWidth="200.0" text="Name"/>
            <TableColumn fx:id="columnSeasons" prefWidth="50.0" text="Seasons"/>
            <TableColumn fx:id="columnEpisodes" prefWidth="56.0" text="Episodes"/>
        </columns>
    </TableView>
    <Label id="biggerLabel" layoutX="126.0" layoutY="14.0" text="Finished Series" underline="true">
        <font>
            <Font size="18.0"/>
        </font>
    </Label>
    <Button fx:id="backButton" layoutX="224.0" layoutY="549.0" mnemonicParsing="false" onAction="#back"
            text="Go back to Main Menu"/>
    <Label id="normalLabel" layoutX="25.0" layoutY="516.0" text="Total time:"/>
    <Label id="normalLabel" fx:id="labelWasted" layoutX="128.0" layoutY="516.0"/>
</AnchorPane>
